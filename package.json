{
  "name": "@vasiliicuhar/invariant.macro",
  "version": "0.1.4",
  "description": "Babel macros for invariants",
  "keywords": [
    "invariant",
    "babel-plugin-macros"
  ],
  "repository": "git@github.com:vasiliicuhar/invariant.macro.git",
  "author": "Vasilii Cuhar <vasiliicuhar@fastmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "jest",
    "build": "webpack build",
    "prepublish": "NODE_ENV=development yarn run build && NODE_ENV=production yarn run build"
  },
  "sideEffects": false,
  "type": "module",
  "main": "./macro.cjs",
  "exports": {
    ".": "./macro.cjs",
    "./invariant": {
      "require": "./invariant.cjs",
      "default": "./invariant.js"
    }
  },
  "types": "./types.d.ts",
  "dependencies": {
    "@babel/runtime": "^7.12.18"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.35",
    "@vasiliicuhar/eslint-preset": "^0.0.4",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.0.1",
    "babel-plugin-tester": "^10.0.0",
    "eslint": "^7.20.0",
    "husky": "^5.0.9",
    "jest": "^26.6.3",
    "lint-staged": "^10.5.4",
    "prettier": "^2.2.1",
    "pretty-quick": "^3.1.0",
    "webpack": "^5.24.0",
    "webpack-cli": "^4.5.0"
  },
  "files": [
    "README.md",
    "types.d.ts",
    "macro.cjs",
    "invariant.js",
    "invariant.cjs",
    "dist"
  ]
}
